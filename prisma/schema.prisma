// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Patient {
  id           String   @id @default(uuid())
  name         String
  registerDate DateTime @default(now())
  
  @@map("patients")
}

